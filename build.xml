<?xml version="1.0" encoding="UTF-8"?>
<project name="Trinity" basedir="." default="buildall">
   <target name="appcompat_v7">
        <ant dir="appcompat_v7" target="clean"/>
        <ant dir="appcompat_v7" target="build"/>
   </target>

   <target name="google-play-services_lib" depends="">
        <ant dir="google-play-services_lib" target="clean"/>
        <ant dir="google-play-services_lib" target="build"/>
   </target>

   <target name="PocketPickup" depends="appcompat_v7, google-play-services_lib">
        <ant dir="PocketPickup" target="clean"/>
        <ant dir="PocketPickup" target="build"/>
   </target>

   <target name="PocketPickupTest" depends="PocketPickup">
        <ant dir="PocketPickupTest" target="clean"/>
        <ant dir="PocketPickupTest" target="build"/>
   </target>
   <target name="buildall" depends="PocketPickup"/>
</project>